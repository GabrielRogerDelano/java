package classe;

public class Produto {
	
	//objetos da classe
	String nome;
	double preco;
	static double desconto = 1;
	
	//construtor
	Produto(String nomeInicial){
		nome = nomeInicial;
	}
	
	static double desconto(){
		return 1;
	}
	
	//metodo de desconto
	double precoComDesconto() {
		return preco * (1 - desconto);
	}
}

package fundamentos;

public class Aritmeticos {
	public static void main(String[] args) {
		System.out.println(2 + 4);
		
		double x = 59.5;
		double y = 12.6;
		

		System.out.println(x + y);
		System.out.println(x - y);
		System.out.println(x * y);
		System.out.println(x / y);
		System.out.println(x % y);
		
		int a = 8;
		int b = 3;
		
		System.out.println(a + b);
		System.out.println(a - b);
		System.out.println(a * b);
		System.out.println(a / b);
		System.out.println(a % b);
		
		
		
	}
}

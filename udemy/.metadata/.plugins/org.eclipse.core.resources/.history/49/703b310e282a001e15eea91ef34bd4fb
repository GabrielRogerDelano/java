package fundamentos;

public class ConversaoTiposPrimitivoNumericos {
	public static void main(String[] args) {
		double a = 1;
		System.out.println(a);
	
		float b =  (float)1.90;
		System.out.println(b);

		int c = 4;
		byte i =  (byte) c ;
		System.out.println(i);
		
		double e = 1.999;
		int f =  (int)e;
		System.out.println(f);
	}
}

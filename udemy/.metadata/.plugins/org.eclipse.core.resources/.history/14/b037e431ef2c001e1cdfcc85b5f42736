package classe;

public class DataTeste {
	public static void main(String[] args) {
		
		Data NiverRobson  = new Data();
		NiverRobson.dia = 29;
		NiverRobson.mes = 2;
		NiverRobson.ano = 2004;
		
		System.out.printf(NiverRobson.obterDataFormatada());

		Data d2  = new Data();
		d2.dia = 29;
		d2.mes = 2;
		d2.ano = 2004;
		
		System.out.printf(d2.obterDataFormatada());
	}
}

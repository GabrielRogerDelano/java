public class If {
	public static void main(String[] args) {
		Scanner entrada = new Scanner(System.in);
		
		System.out.println("informe a media");
		double media = entrada.nextDouble();
		
		if( media >= 7) {
			System.out.println("Aluno aprovado");
			System.out.println("Parab√©ns");
		}
		
		
		entrada.close();
	}
}

package classe;

public class ValorVsReferencia {
	
	public static void main(String[] args) {
		
		int a = 2;
		int b = a; // atribuicao por valor
		
		a ++;
		b --;
		
		System.out.println(a + " " + b);
		
		Data d1 = new Data();
		Data d2 = d1;
		
		d2.mes = 9;
		d2.ano = 2004;
		
		System.out.println(d1.obterDataFormatada());
		System.out.println(d2.obterDataFormatada());
	}
}
